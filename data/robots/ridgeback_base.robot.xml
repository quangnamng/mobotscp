<?xml version="1.0" encoding="utf-8"?>
<robot name="ridgeback_base">

  <!-- chassis -->
  <kinbody>
    <mass type="custom">
      <translation>0.012 0.002 0.067</translation>
      <!--<com>0.012 0.002 0.067</com>-->
      <total>138</total>
      <inertia>4.4744 0.03098 0.003647 0.03098 7.1624 0.1228 0.003647 0.1228 4.6155</inertia>
    </mass>
    <body name="chassis_link" type="dynamic">
      <geom type="trimesh" modifiable="true">
        <diffuseColor>0.15 0.15 0.15</diffuseColor>
        <!--<transparency>0.5</transparency>-->
        <render>meshes/ridgeback_base/body.stl 1.0</render>
        <collision>meshes/ridgeback_base/body-collision.stl 1.0</collision>
        <translation>0 0 0.015</translation>
      </geom>
    </body>
  </kinbody>

  <!-- left side cover -->
  <kinbody>
    <mass>
      <total>0.0</total>
    </mass>
    <body name="left_side_cover_link" type="dynamic">
      <geom type="trimesh" modifiable="true">
        <RotationAxis>0 0 1 180</RotationAxis>
        <diffuseColor>0.8 0.8 0.0</diffuseColor>
        <render>meshes/ridgeback_base/side-cover.stl 1.0</render>
        <collision>meshes/ridgeback_base/side-cover.stl 1.0</collision>
        <translation>0 0 0.015</translation>
      </geom>
    </body>
    <joint name="left_side_cover_link_joint" type="hinge" enable="false">
      <body>chassis_link</body>
      <body>left_side_cover_link</body>
      <limits>0 0</limits>
    </joint>
  </kinbody>

  <!-- right side cover -->
  <kinbody>
