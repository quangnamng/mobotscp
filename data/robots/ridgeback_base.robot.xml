  <!-- front rocker + front left wheel + front right wheel -->
  <kinbody>
    <!-- front rocker -->
    <body name="front_rocker_link" type="dynamic">
      <mass type="custom">
        <RotationAxis>1 0 0 90</RotationAxis>
        <RotationAxis>0 0 1 90</RotationAxis>
        <!--<total>10.267</total>-->
        <total>0</total>
        <inertia>0.0288 2.20484e-6 -1.3145e-5 2.20484e-6 0.4324 1.8944e-3 -1.3145e-5 1.8944e-3 0.4130</inertia>
        <com>0 0 0</com>
      </mass>
      <geom type="trimesh" modifiable="true">
        <RotationAxis>1 0 0 90</RotationAxis>
        <RotationAxis>0 0 1 90</RotationAxis>
        <diffuseColor>0.15 0.15 0.15</diffuseColor>
        <!--<transparency>0.5</transparency>-->
        <render>meshes/ridgeback_base/rocker.stl 1.0</render>
        <collision>meshes/ridgeback_base/rocker.stl 1.0</collision>
        <translation>0 0 0.015</translation>
      </geom>
      <offsetfrom>axle_link</offsetfrom>
      <translation>0.319 0 0</translation>
    </body>
    <joint name="front_rocker" type="hinge" enable="false">
      <body>axle_link</body>
      <body>front_rocker_link</body>
      <limits>0 0</limits>
    </joint>

    <!-- front left wheel -->
    <body name="front_left_wheel_link" type="dynamic">
      <mass type="custom">
        <!--<total>2.3</total>-->
        <total>0.0</total>
        <inertia>3.3212e-3 0 0 0 6.6424e-3 0 0 0 3.3212e-3</inertia>
        <com>0 0 0</com>
      </mass>
      <geom type="cylinder">
        <!--<RotationAxis>1 0 0 90</RotationAxis>-->
        <diffuseColor>0.15 0.15 0.15</diffuseColor>
        <height>0.0790</height>
        <radius>0.065</radius>
        <!--<render>meshes/ridgeback_base/wheel.stl 1.0</render>-->
        <!--<collision>meshes/ridgeback_base/wheel.stl 1.0</collision>-->
        <translation>0 0 0.015</translation>
      </geom>
      <offsetfrom>front_rocker_link</offsetfrom>
      <translation>0 0.2755 0</translation>
    </body>
    <joint name="front_left_wheel" type="hinge" enable="false">
      <body>front_rocker_link</body>
      <body>front_left_wheel_link</body>
      <limits>0 0</limits>
    </joint>

